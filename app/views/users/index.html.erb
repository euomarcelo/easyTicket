<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Sign up', new_user_registration_path %>